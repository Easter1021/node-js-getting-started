[  {    "type": "get",    "url": "/translate",    "title": "translate text",    "name": "get",    "group": "Translate",    "version": "0.1.0",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "text",            "description": "<p>origin content</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": true,            "field": "from",            "defaultValue": "auto",            "description": "<p>origin country code, see <a href=\"/support_lang\" target=\"_blank\">language list</a></p>"          },          {            "group": "Parameter",            "type": "String",            "optional": true,            "field": "to",            "defaultValue": "en",            "description": "<p>target country code, see <a href=\"/support_lang\" target=\"_blank\">language list</a></p>"          }        ]      },      "examples": [        {          "title": "Example",          "content": "{\n    text: \"Ik spreek Nederlands!\",\n    from: \"nl\",\n    to: \"en\",\n}",          "type": "json"        }      ]    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Boolean",            "optional": false,            "field": "success",            "description": "<p>is it done.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "text",            "description": "<p>target content</p>"          }        ]      },      "examples": [        {          "title": "Succsess Reponse:",          "content": "HTTP/1.1 200 OK\n{\n       success: true,\n       success: \"I spea Dutch!\"\n}",          "type": "json"        }      ]    },    "error": {      "fields": {        "Error 4xx": [          {            "group": "Error 4xx",            "optional": false,            "field": "error",            "description": "<p>Something Error Message</p>"          }        ]      },      "examples": [        {          "title": "406 Response:",          "content": "HTTP/1.1 406 Not Acceptable\n{\n  error: \"No text input!\"\n}",          "type": "json"        },        {          "title": "500 Response:",          "content": "HTTP/1.1 500 Internal Server Error\nSomething broke!",          "type": "json"        }      ]    },    "filename": "routes/api/translate.js",    "groupTitle": "Translate"  }]
